<p><h2>
	<strong>Skate Video:</strong>
	<%= @skate_video.videoname %>
</h2></p>

<p>
	<strong>Year:</strong>
	<%= @skate_video.videoyear %>
</p>

<p>
	<strong>Company:</strong>
	<%= @skate_video.videocompany %>
</p>

<h2>Video Parts</h2>
<% @skate_video.video_parts.each do |video_part| %>
	<p>
		<strong>Skate Video:</strong>
		<%= video_part.videoname %>
	</p>
	<p>
		Artist: <%= video_part.artist %>
	</p>
	<p>
		Song: <%= video_part.song %>
	</p>
	<p>
		Length: <%= video_part.runtime %>
	</p>
<% end %>

<h2>Add New Video Part</h2>
<%= form_for([@skate_video, @skate_video.video_parts.build]) do |f| %>
	<p>
		<%= f.label :videoname %><br>
		<%= f.text_field :videoname %>
	</p>
	<p>
		<%= f.label :artist %><br>
		<%= f.text_field :artist %>
	</p>
	<p>
		<%= f.label :song %><br>
		<%= f.text_field :song %>
	</p>
	<p>
		<%= f.label :runtime %><br>
		<%= f.text_field :runtime %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<% end %>
<div align='center'>
	<button type="button" class="btn btn-sm" %>
		<%= link_to 'Back', skate_videos_path %>
	</button>
</div>